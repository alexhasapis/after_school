<p id="notice"><%= notice %></p>

<h1>Listing Performances</h1>

<table>
  <thead>
    <tr>
      <th>Dbn</th>
      <th>On track 2013</th>
      <th>Grad rate 2013</th>
      <th>College rate 2013</th>
      <th>Student satisfaction</th>
      <th>On track 2012</th>
      <th>Grad rate 2012</th>
      <th>College rate 2012</th>
      <th>Student satisfation 2012</th>
      <th>On track sim schools</th>
      <th>Grad rate sim schools</th>
      <th>College rate sim schools</th>
      <th>Student satisfaction sim schools</th>
      <th>Quailty review</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @performances.each do |performance| %>
      <tr>
        <td><%= performance.dbn %></td>
        <td><%= performance.on_track_2013 %></td>
        <td><%= performance.grad_rate_2013 %></td>
        <td><%= performance.college_rate_2013 %></td>
        <td><%= performance.student_satisfaction %></td>
        <td><%= performance.on_track_2012 %></td>
        <td><%= performance.grad_rate_2012 %></td>
        <td><%= performance.college_rate_2012 %></td>
        <td><%= performance.student_satisfation_2012 %></td>
        <td><%= performance.on_track_sim_schools %></td>
        <td><%= performance.grad_rate_sim_schools %></td>
        <td><%= performance.college_rate_sim_schools %></td>
        <td><%= performance.student_satisfaction_sim_schools %></td>
        <td><%= performance.quality_review %></td>
        <td><%= link_to 'Show', performance %></td>
        <td><%= link_to 'Edit', edit_performance_path(performance) %></td>
        <td><%= link_to 'Destroy', performance, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Performance', new_performance_path %>
